<p id="notice"><%= notice %></p>

<h1>Ticket Priorities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_priorities.each do |ticket_priority| %>
      <tr>
        <td><%= link_to 'Show', ticket_priority %></td>
        <td><%= link_to 'Edit', edit_ticket_priority_path(ticket_priority) %></td>
        <td><%= link_to 'Destroy', ticket_priority, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Priority', new_ticket_priority_path %>
