<p id="notice"><%= notice %></p>

<h1>Custom Validation Rules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @custom_validation_rules.each do |custom_validation_rule| %>
      <tr>
        <td><%= link_to 'Show', custom_validation_rule %></td>
        <td><%= link_to 'Edit', edit_custom_validation_rule_path(custom_validation_rule) %></td>
        <td><%= link_to 'Destroy', custom_validation_rule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custom Validation Rule', new_custom_validation_rule_path %>
