<p id="notice"><%= notice %></p>

<h1>Sla Associations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sla_associations.each do |sla_association| %>
      <tr>
        <td><%= link_to 'Show', sla_association %></td>
        <td><%= link_to 'Edit', edit_sla_association_path(sla_association) %></td>
        <td><%= link_to 'Destroy', sla_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla Association', new_sla_association_path %>
