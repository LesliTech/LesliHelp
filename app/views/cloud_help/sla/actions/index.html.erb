<p id="notice"><%= notice %></p>

<h1>Sla Actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sla_actions.each do |sla_action| %>
      <tr>
        <td><%= link_to 'Show', sla_action %></td>
        <td><%= link_to 'Edit', edit_sla_action_path(sla_action) %></td>
        <td><%= link_to 'Destroy', sla_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla Action', new_sla_action_path %>
