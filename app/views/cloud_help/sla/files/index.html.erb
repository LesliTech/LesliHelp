<p id="notice"><%= notice %></p>

<h1>Sla Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sla_files.each do |sla_file| %>
      <tr>
        <td><%= link_to 'Show', sla_file %></td>
        <td><%= link_to 'Edit', edit_sla_file_path(sla_file) %></td>
        <td><%= link_to 'Destroy', sla_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla File', new_sla_file_path %>
