<p id="notice"><%= notice %></p>

<h1>Sla Subscribers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sla_subscribers.each do |sla_subscriber| %>
      <tr>
        <td><%= link_to 'Show', sla_subscriber %></td>
        <td><%= link_to 'Edit', edit_sla_subscriber_path(sla_subscriber) %></td>
        <td><%= link_to 'Destroy', sla_subscriber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla Subscriber', new_sla_subscriber_path %>
