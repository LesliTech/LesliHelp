<p id="notice"><%= notice %></p>

<h1>Sla Activities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sla_activities.each do |sla_activity| %>
      <tr>
        <td><%= link_to 'Show', sla_activity %></td>
        <td><%= link_to 'Edit', edit_sla_activity_path(sla_activity) %></td>
        <td><%= link_to 'Destroy', sla_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla Activity', new_sla_activity_path %>
