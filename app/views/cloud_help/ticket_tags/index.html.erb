<p id="notice"><%= notice %></p>

<h1>Ticket Tags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_tags.each do |ticket_tag| %>
      <tr>
        <td><%= link_to 'Show', ticket_tag %></td>
        <td><%= link_to 'Edit', edit_ticket_tag_path(ticket_tag) %></td>
        <td><%= link_to 'Destroy', ticket_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Tag', new_ticket_tag_path %>
