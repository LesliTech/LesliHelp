<p id="notice"><%= notice %></p>

<h1>Catalog Ticket Sources</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalog_ticket_sources.each do |catalog_ticket_source| %>
      <tr>
        <td><%= link_to 'Show', catalog_ticket_source %></td>
        <td><%= link_to 'Edit', edit_catalog_ticket_source_path(catalog_ticket_source) %></td>
        <td><%= link_to 'Destroy', catalog_ticket_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

