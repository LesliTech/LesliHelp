<p id="notice"><%= notice %></p>

<h1>Workflow Statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_statuses.each do |workflow_status| %>
      <tr>
        <td><%= link_to 'Show', workflow_status %></td>
        <td><%= link_to 'Edit', edit_workflow_status_path(workflow_status) %></td>
        <td><%= link_to 'Destroy', workflow_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow Status', new_workflow_status_path %>
