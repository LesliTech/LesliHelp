<p id="notice"><%= notice %></p>

<h1>Workflow Associations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_associations.each do |workflow_association| %>
      <tr>
        <td><%= link_to 'Show', workflow_association %></td>
        <td><%= link_to 'Edit', edit_workflow_association_path(workflow_association) %></td>
        <td><%= link_to 'Destroy', workflow_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow Association', new_workflow_association_path %>
