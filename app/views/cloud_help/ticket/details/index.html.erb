<p id="notice"><%= notice %></p>

<h1>Ticket Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_details.each do |ticket_detail| %>
      <tr>
        <td><%= link_to 'Show', ticket_detail %></td>
        <td><%= link_to 'Edit', edit_ticket_detail_path(ticket_detail) %></td>
        <td><%= link_to 'Destroy', ticket_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Detail', new_ticket_detail_path %>
