<p id="notice"><%= notice %></p>

<h1>Ticket Timelines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_timelines.each do |ticket_timeline| %>
      <tr>
        <td><%= link_to 'Show', ticket_timeline %></td>
        <td><%= link_to 'Edit', edit_ticket_timeline_path(ticket_timeline) %></td>
        <td><%= link_to 'Destroy', ticket_timeline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Timeline', new_ticket_timeline_path %>
