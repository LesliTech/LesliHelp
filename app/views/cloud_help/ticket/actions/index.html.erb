<p id="notice"><%= notice %></p>

<h1>Ticket Actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_actions.each do |ticket_action| %>
      <tr>
        <td><%= link_to 'Show', ticket_action %></td>
        <td><%= link_to 'Edit', edit_ticket_action_path(ticket_action) %></td>
        <td><%= link_to 'Destroy', ticket_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Action', new_ticket_action_path %>
